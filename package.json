{
  "name": "maintenance-tracker",
  "version": "1.0.0",
  "description": "A company app that helps employees forward maintenance/repair requests to the maintenance department",
  "main": "app.js",
  "scripts": {
    "test": "nyc --reporter=text --reporter=lcov mocha --compilers js:babel-core/register ./server/test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "nodemon app.js --exec babel-node  --presets es2015",
    "lint": "eslint **/*.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emekafredy/maintenance-tracker.git"
  },
  "keywords": [
    "node",
    "express"
  ],
  "author": "Emeka Samuel Chinedu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emekafredy/maintenance-tracker/issues"
  },
  "babel": {
    "presets": [
      "stage-3",
      "latest"
    ]
  },
  "homepage": "https://github.com/emekafredy/maintenance-tracker#readme",
  "dependencies": {
    "babel-plugin-istanbul": "^4.1.6",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "express": "^4.16.3",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^8.2.1",
    "logger": "0.0.1",
    "mocha": "^3.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.4",
    "nyc": "^11.8.0",
    "request": "^2.86.0",
    "should": "^13.2.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1"
  }
}
