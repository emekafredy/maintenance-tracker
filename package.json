{
  "name": "maintenance-tracker",
  "version": "1.0.0",
  "description": "A company app that helps employees forward maintenance/repair requests to the maintenance department",
  "main": "app.js",
  "scripts": {
    "test": "npm run mig && nyc --reporter=text --reporter=lcov mocha --compilers js:babel-core/register ./server/test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "nodemon app.js --exec babel-node  --presets es2015",
    "mig": "psql -U postgres -f  ./server/models/tracker.sql",
    "lint": "eslint **/*.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emekafredy/maintenance-tracker.git"
  },
  "keywords": [
    "node",
    "express"
  ],
  "author": "Emeka Samuel Chinedu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emekafredy/maintenance-tracker/issues"
  },
  "babel": {
    "presets": [
      "stage-3",
      "latest"
    ]
  },
  "homepage": "https://github.com/emekafredy/maintenance-tracker#readme",
  "dependencies": {
    "babel-plugin-istanbul": "^4.1.6",
    "body-parser": "^1.18.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "node-fetch": "^2.1.2",
    "nodemon": "^1.17.4",
    "validator": "^10.2.0",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1"
  }
}
