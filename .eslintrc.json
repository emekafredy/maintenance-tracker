{
  "extends": "airbnb-base",
  "rules" : {
      "func-names": ["error", "never"],
      "no-unused-vars": 0,
      "import/no-dynamic-require": 0,
      "no-param-reassign": [2, { "props": false }]
  },
  "globals": {
      "window": true,
      "document": true,
      "location": true,
      "describe": true,
      "it": true
  }
}